{
  "multipart": [
    {
      "when": {
        "has_node": true
      },
      "apply": {
        "model": "arknights_endfield:block/pipe_node"
      }
    },
    {
      "when": {
        "north": true
      }, "apply": {
      "model": "arknights_endfield:block/pipe_side"
      }
    },
    {
      "when": {
        "south": true
      }, "apply": {
      "model": "arknights_endfield:block/pipe_side",
      "y": 180
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "arknights_endfield:block/pipe_side",
        "y": 90
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "arknights_endfield:block/pipe_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "arknights_endfield:block/pipe_side",
        "x": 270
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "arknights_endfield:block/pipe_side",
        "x": 90
      }
    }
  ]
}